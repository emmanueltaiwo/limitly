{
  "name": "limitly-sdk",
  "version": "1.4.1",
  "description": "Official SDK for Limitly rate limiting service",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rate-limit",
    "rate-limiting",
    "token-bucket",
    "redis",
    "sdk"
  ],
  "author": "Emmanuel Taiwo",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "posthog-node": "^5.21.1",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmanueltaiwo/limitly.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
